{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "hishamss/go-meeting-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "api",
            "image": "hishamss/go-meeting-api",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "hishamss/go-meeting-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "api"]
        }
    ]
}